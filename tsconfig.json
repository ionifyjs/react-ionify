{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "types": ["react", "react-dom"],
    "baseUrl": ".",
    "paths": {
      "@core/*": ["./core/*"]
    }
  },
  "include": ["src", "core"]
}
